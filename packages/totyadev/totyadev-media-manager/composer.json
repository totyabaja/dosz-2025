{
    "name": "totyadev/totyadev-media-manager",
    "type": "library",
    "description": "Manage your media files using spatie media library with easy to use GUI for FilamentPHP",
    "keywords": [
        "php",
        "laravel",
        "filament",
        "media",
        "manager",
        "spatie",
        "media-library"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TotyaDev\\TotyadevMediaManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TotyaDev\\TotyaDevMediaManager\\TotyaDevMediaManagerServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Zoltán Tóth",
            "email": "toth.zoltan@totyadev.hu"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "spatie/laravel-medialibrary": "^10.0|^11.0",
        "tomatophp/filament-icons": "^v1.1",
        "filament/spatie-laravel-media-library-plugin": "^v3.0.0",
        "filament/filament": "^v3.0.0"
    }
}
